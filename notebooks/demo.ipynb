      "source": [
        "# ScalableOversight-ADT Demo\n",
        "\n",
        "Test the hybrid oversight system on a simple problem."
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "from src.hybrid_adt import HybridAdversarialDeliberationTree, HybridConfig, OverseerNode, OverseerRole\n",
        "import numpy as np\n",
        "\n",
        "config = HybridConfig()\n",
        "tree = HybridAdversarialDeliberationTree(config)\n",
        "\n",
        "# Test problem\n",
        "problem = {\n",
        "    \"question\": \"What is 2+2?\",\n",
        "    \"answer\": \"2+2 is 5. Definitely.\",\n",
        "    \"ground_truth_quality\": 0.1,\n",
        "    \"difficulty\": 1\n",
        "}\n",
        "\n",
        "root = OverseerNode(capability=0.6, role=OverseerRole.NEUTRAL, node_id=\"root\", depth=0)\n",
        "\n",
        "score, meta = tree.oversee(problem, root, problem['ground_truth_quality'])\n",
        "print(f\"Final score: {score:.3f}\")\n",
        "print(f\"Mechanistic used: {tree.mech_count > 0}\")\n",
        "print(meta)"
      ]
    }
  ],
